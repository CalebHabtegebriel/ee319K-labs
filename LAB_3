;			STR R2, [R1]				;Store back into mem

;Exhale30_ON	SUBS R0, R0, #0x01			;Subtract 1
;			BNE Exhale30_ON				;If not 0, repeat
;
;			LDR R0, =DutyCycle_70b		;R0 = # bus cycles for 0.35 seconds
;			AND R2, #0x02				;Turn PE2 off
;			STR R2, [R1]				;Store into mem
						
						;Bottom reisstor prevesnts floating values 
;Exhale30_OFF	SUBS R0, R0, #0x01			;Subtract 1
;			BNE Exhale30_OFF				;If not 0, repeat
;			
;			LDR R0, =DutyCycle_50b		;R0 = # bus cycles for 0.25 seconds
;			LDR R1, =GPIO_PORTE_DATA_R	;R1 = Port E data address
;			LDR R2, [R1]				;R2 = Port E data
;			ORR R2, #0x04				;Turn PE2 on
;			STR R2, [R1]				;Store back into mem
			
;Exhale10_ON	SUBS R0, R0, #0x01			;Subtract 1
;			BNE Exhale10_ON				;If not 0, repeat
;
;			LDR R0, =DutyCycle_90b		;R0 = # bus cycles for 0.45 seconds
;			AND R2, #0x02				;Turn PE2 off
;			STR R2, [R1]				;Store into mem
			
;Exhale10_OFF	SUBS R0, R0, #0x01			;Subtract 1
;			BNE Exhale10_OFF				;If not 0, repeat
;			
;			LDR R0, =GPIO_PORTF_DATA_R	;R0 = Port F data address
;			LDR R1, [R0]				;R1 = Port F data
;			LSR R1, #4					;Put input into LSB
;			AND R1, #1					;Clear all bits except for input which is in LSB
;			ADD R1, #-1					;Check to see if Breath button is on
;			BNE	Check_Counter
;			B Breath

Breath
			LDR R5, =DutyCycleb		;R0 = # bus cycles for 0.15 seconds
			LDR R3, =DutyCycle_max;
Breath2		LDR R1, =GPIO_PORTE_DATA_R	;R1 = Port E data address
			LDR R2, [R1]				;R2 = Port E data
			ORR R2, #0x04				;Turn PE2 on
			STR R2, [R1]				;Store back into mem
			ADD R5, #0X07D0;
			MOV R0, R5;
			CMP R0, R3;
			BEQ Exhale
			
			
Breath_ON	SUBS R0, R0, #0x01			;Subtract 1
			BNE Breath_ON				;If not 0, repeat

			LDR R3, =DutyCycle_max		;R0 = # bus cycles for 0.35 seconds
			SUBS R6, R3, R5 
			AND R2, #0x02				;Turn PE2 off
			STR R2, [R1]				;Store into mem
						
						;Bottom reisstor prevesnts floating values 
Breath_OFF	SUBS R6, R6, #0x01			;Subtract 1
			BNE Breath_OFF				;If not 0, repeat
			
			LDR R0, =GPIO_PORTF_DATA_R	;R0 = Port F data address
			LDR R1, [R0]				;R1 = Port F data
			LSR R1, #4					;Put input into LSB
			AND R1, #1					;Clear all bits except for input which is in LSB
			CMP R1, #1
			;ADD R1, #-1					;Check to see if Breath button is on
			BEQ	Check_Counter
			B 	Breath2

Exhale		LDR R5, =DutyCycle_max		;R0 = # bus cycles for 0.15 seconds
			LDR R3, =DutyCycleb;
Breath3		LDR R1, =GPIO_PORTE_DATA_R	;R1 = Port E data address
			LDR R2, [R1]				;R2 = Port E data
			ORR R2, #0x04				;Turn PE2 on
			STR R2, [R1]				;Store back into mem
			SUBS R5, #0X07D0;
			MOV R0, R5
			CMP R0, R3;
			BEQ Breath
			
			
Breath2_ON	SUBS R0, R0, #0x01			;Subtract 1
			BNE Breath2_ON				;If not 0, repeat

			LDR R3, =DutyCycle_max	;R0 = # bus cycles for 0.35 seconds
			SUBS R6, R3, R5
			AND R2, #0x02				;Turn PE2 off
			STR R2, [R1]				;Store into mem
						
						;Bottom reisstor prevesnts floating values 
Breath2_OFF	SUBS R6, R6, #0x01			;Subtract 1
			BNE Breath2_OFF				;If not 0, repeat
			
			LDR R0, =GPIO_PORTF_DATA_R	;R0 = Port F data address
			LDR R1, [R0]				;R1 = Port F data
			LSR R1, #4					;Put input into LSB
			AND R1, #1					;Clear all bits except for input which is in LSB
			CMP R1, #1
			;ADD R1, #-1					;Check to see if Breath button is on
			BEQ	Check_Counter
			B Breath3


     ALIGN      ; make sure the end of this section is aligned
     END        ; end of file
